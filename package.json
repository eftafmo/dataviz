{
  "name": "dataviz",
  "version": "0.9.0",
  "description": "EEA & Norway grants data and results portal",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eftfmo/dataviz.git"
  },
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "dev": "vite",
    "lint": "run-s lint:css lint:js",
    "lint:check": "run-s lint:css:check lint:js:check",
    "lint:js": "eslint --fix \"assets/**/*.js\" \"assets/**/*.vue\"",
    "lint:js:check": "eslint \"assets/**/*.js\" \"assets/**/*.vue\"",
    "lint:css": "prettier --write -c assets/css/",
    "lint:css:check": "prettier --check -c assets/css/",
    "build": "vite build",
    "serve": "vite preview"
  },
  "browserslist": [
    "IE >= 9",
    "last 2 versions",
    "> 5%"
  ],
  "dependencies": {
    "clipboard": "^2.0.11",
    "d3": "^7.9.0",
    "d3-tip": "^0.9.1",
    "lodash.debounce": "^4.0.8",
    "lodash.merge": "^4.6.2",
    "lodash.orderby": "^4.6.0",
    "normalize.css": "^8.0.1",
    "popper.js": "^1.16.1",
    "vite-plugin-json5": "^1.1.2",
    "vue": "^3.4.31",
    "vue3-tabs-component": "^1.3.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.6.0",
    "@rollup/pluginutils": "^5.1.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/compiler-sfc": "^3.4.31",
    "autoprefixer": "^10.4.19",
    "classlist-polyfill": "^1.2.0",
    "es6-promise": "^4.2.8",
    "form-association-polyfill": "^2.0.3",
    "glob": "^10.4.3",
    "json5": "^2.2.3",
    "less": "^4.2.0",
    "topojson-client": "^3.1.0",
    "url-polyfill": "^1.1.12",
    "vite": "^5.3.3",
    "eslint": "^9.6.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier-vue": "^5.0.0",
    "eslint-plugin-vue": "^9.27.0",
    "lint-staged": "^15.2.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.2",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{less,css,scss}": [
      "prettier -c --write"
    ],
    "*.{js,jsx,vue}": [
      "eslint --fix"
    ]
  }
}
